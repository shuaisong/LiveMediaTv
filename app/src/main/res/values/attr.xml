<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawEndEditText">
        <attr name="is_password" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ShangshabanChangeTextSpaceView">
        <attr name="space" format="float" />
    </declare-styleable>
    <declare-styleable name="CheckableVipLinearLayout">
        <attr name="isChecked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TitleView">
        <attr name="title" format="string" />
        <attr name="backImg" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="integer" />
    </declare-styleable>
    <declare-styleable name="CustomPraiseView">
        <attr name="checked_bg" />
        <attr name="unchecked_bg" />
    </declare-styleable>

    <declare-styleable name="WaveView">

        <attr name="waveColor" format="color" />

        <attr name="baselineColor" format="color" />

        <attr name="maxValue" format="integer" />

        <attr name="waveStokeWidth" format="dimension" />

        <attr name="invalidateTime" format="integer" />

        <attr name="dividerSpace" format="dimension" />

    </declare-styleable>

    <declare-styleable name="CheckableImageView">
        <attr name="checkedImg" format="reference" />
        <attr name="unCheckedImg" format="reference" />
        <attr name="tintColor" format="color" />
        <attr name="checked" format="boolean" />
    </declare-styleable>
